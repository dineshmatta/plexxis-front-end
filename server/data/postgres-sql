CREATE TABLE users (
  ID SERIAL PRIMARY KEY,
  name VARCHAR(30),
  code VARCHAR(30),
  profession VARCHAR(30),
  color VARCHAR(30),
  city VARCHAR(30),
  branch VARCHAR(30),
  assigned Boolean
);

